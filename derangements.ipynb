    "def make_permutations(group, out_param=[]):\n",
    "    if len(group) == 0:\n",
    "        return out_param\n",
    "    first = group.pop(0)\n",
    "    remaining = group\n",
    "    for i in remaining:\n",
    "        out_param.append(first + i)\n",
    "    return make_permutations(remaining, out_param)\n",
    "\n",
    "def permute(data,n):\n",
    "    if n==1: \n",
    "        return data\n",
    "    else:\n",
    "        arr = []\n",
    "        for i in range(0, n-1):\n",
    "            arr.extend(permute(data, n-1))\n",
    "            if (n%2): \n",
    "                data[0], data[n-1] = data[n-1],data[0]\n",
    "            else: data[i], data[n-1] = data[n-1], data[i]\n",
    "        arr.extend(permute(data, n-1))\n",
    "        return arr\n",
    "\n",
    "def derange(group:list):\n",
    "    permutations = permute(group, len(group))\n",
    "    derangements = [it for it in permutations if all([it[i] != group[i] for i in range(len(group))])]\n",
    "    return derangements\n",
    "\n",
    "def double_derange(group:list):\n",
    "    initial = derange(group)\n",
    "    ddrs = {}\n",
    "    for dgn in initial:\n",
    "        second = derange(dgn)\n",
    "        second = [dg for dg in second if all([dg[i] != dgn[i] and dg[i] != group[i] for i in range(len(group))])]\n",
    "        ddrs[dgn] = (second)\n",
    "        #     second = list(filter(lambda it: all([it[i] != initial[i] and it[i] != group[i] for i in range(len(group))]), derange(initial)))\n",
    "    return ddrs\n",
    "            "
